{
  "name": "arbutus",
  "description": "Documentation site UI library",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/arbutus.git"
  },
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "components/*",
    "hooks/*",
    "styles/*",
    "utilities/*"
  ],
  "scripts": {
    "build:packages": "lage build",
    "build:storybook": "build-storybook -o docs",
    "change": "beachball change",
    "g:component": "run-s hygen:component packages:fix-mismatches i",
    "g:hook": "run-s hygen:hook packages:fix-mismatches i",
    "hygen:component": "hygen new component",
    "hygen:hook": "hygen new hook",
    "i": "npm i",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write .",
    "lint:root": "run-s lint:eslint lint:prettier",
    "lint:staged": "lint-staged",
    "lint": "lage lint",
    "syncpack:check": "syncpack list-mismatches",
    "syncpack:fix": "syncpack fix-mismatches",
    "beachball:publish": "beachball publish",
    "publish:packages": "run-s build:packages beachball:publish",
    "purge": "git clean -d -f -X -e .env -e .husky",
    "reset": "run-s purge i build:packages",
    "start": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@griffel/react": "^1.4.1",
    "@microsoft/arbutus.aside-navigation": "*",
    "@microsoft/arbutus.bookmark-tile": "*",
    "@microsoft/arbutus.divider": "*",
    "@microsoft/arbutus.icon-button": "*",
    "@microsoft/arbutus.icon": "*",
    "@microsoft/arbutus.link": "*",
    "@microsoft/arbutus.main-navigation": "0.0.2",
    "@microsoft/arbutus.mark-list": "*",
    "@microsoft/arbutus.recommendation-tile": "*",
    "@microsoft/arbutus.tabs": "*",
    "@microsoft/arbutus.text": "*",
    "@microsoft/arbutus.theming": "*",
    "@microsoft/arbutus.tile": "*",
    "@microsoft/arbutus.ts": "*",
    "@microsoft/arbutus.use-space-styles": "0.0.1",
    "@rnx-kit/eslint-plugin": "^0.2.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "assert": "^2.0.0",
    "beachball": "^2.31.4",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "hygen": "^6.2.11",
    "lage": "^1.9.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "syncpack": "^8.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "wait-on": "6.0.1",
    "webpack": "^5.74.0"
  },
  "syncpack": {
    "semverRange": [
      "<=",
      ">="
    ],
    "workspace": true,
    "peer": false
  },
  "resolutions": {
    "**/trim": "^1.0.0"
  },
  "engines": {
    "node": "16.15.1"
  }
}
