{
  "name": "@microsoft/arbutus.prop-table",
  "version": "0.0.5",
  "description": "PropTable is a composite component that generates documentation for React components.",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": [
    "./lib/index.js"
  ],
  "files": [
    "lib/**"
  ],
  "sideEffects": false,
  "scripts": {
    "arbutus-prop-docs:clean": "arbutus-prop-docs clean",
    "arbutus-prop-docs:generate": "arbutus-prop-docs generate --ts && eslint __prop-docs__ --fix",
    "build": "tsc",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <=18.2.0",
    "react-dom": ">=16.8.0 <=18.2.0"
  },
  "dependencies": {
    "@fluentui/react-theme": "^9.1.0",
    "@griffel/react": "^1.5.2",
    "@microsoft/arbutus.table-list": "^0.1.0",
    "@microsoft/arbutus.use-space-styles": "^0.0.5",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@microsoft/arbutus.prop-docs-cli": "^0.1.0",
    "@microsoft/arbutus.ts": "0.0.2",
    "typescript": "^4.9.4"
  }
}
