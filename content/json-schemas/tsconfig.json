  "compilerOptions": {
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "module": "CommonJS",
    "target": "ES6",
    "jsx": "react",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules/*"
  ]
}